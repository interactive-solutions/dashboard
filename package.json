{
  "name": "nextjs-boilerplate",
  "version": "1.3.37",
  "private": true,
  "author": "gunnarx2",
  "license": "MODO",
  "scripts": {
    "start": "next dev -p 9000",
    "start:localhost": "env-cmd -f .env.isLocalhost next dev -p 9000",
    "start:staging": "env-cmd -f .env.isStaging next dev -p 9000",
    "start:production": "env-cmd -f .env.isProduction next dev -p 9000",
    "build": "next build",
    "build:localhost": "env-cmd -f .env.isLocalhost next build",
    "build:staging": "env-cmd -f .env.isStaging next build",
    "build:production": "env-cmd -f .env.isProduction next build",
    "serve": "next start -p 9090",
    "serve:localhost": "env-cmd -f .env.isLocalhost next start -p 9090",
    "serve:staging": "env-cmd -f .env.isStaging next start -p 9090",
    "serve:production": "env-cmd -f .env.isProduction next start -p 9090",
    "prettier:report": "prettier '**/*.{js,tsx,ts,scss,json,md}' --check",
    "prettier:fix": "prettier '**/*.{js,tsx,ts,scss,json,md}' --write",
    "eslint:report": "eslint '**/*.{js,tsx,ts}' --cache",
    "eslint:fix": "eslint '**/*.{js,tsx,ts}' --fix",
    "stylelint:report": "stylelint '**/*.{js,jsx,ts,tsx}' --cache",
    "stylelint:fix": "stylelint '**/*.{js,jsx,ts,tsx}' --fix",
    "typescript:report": "tsc --noEmit && tsc --noEmit",
    "graphql:generate": "graphql-codegen --config codegen/default.config.ts && yarn graphql:operations && graphql-codegen --config codegen/everything.config.ts",
    "graphql:watch": "graphql-codegen --config codegen/default.config.ts --watch",
    "graphql:operations": "amplify-graphql-docs-generator --schema src/graphql/cli/schema.everything.json --maxDepth=4 --output src/graphql/cli/operations.everything.graphql --language graphql",
    "i18n:extract": "formatjs extract 'src/**/*.{js,tsx,ts}' --out-file src/i18n/cli/extracted.json --ignore **/*.d.ts",
    "i18n:compile": "formatjs compile src/i18n/cli/extracted.json --out-file src/i18n/cli/compiled.json"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@entire.se/hooks": "^1.3.2",
    "@entire.se/utils": "^1.1.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.9",
    "@mui/x-date-pickers": "^6.0.4",
    "@tanstack/react-table": "^8.7.9",
    "clsx": "^1.2.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^10.5.0",
    "graphql": "16.6.0",
    "next": "^13.1.6",
    "next-query-params": "^4.1.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.2.8",
    "use-query-params": "^2.2.0",
    "validator": "^13.9.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@calm/eslint-plugin-react-intl": "^1.4.1",
    "@entire.se/eslint-config-eslint-react": "^2.2.0",
    "@entire.se/eslint-plugin-eslint-react": "^1.2.0",
    "@entire.se/prettier": "^2.0.0",
    "@entire.se/stylelint-emotion": "^1.0.1",
    "@formatjs/cli": "^6.0.1",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/fragment-matcher": "^4.0.0",
    "@graphql-codegen/introspection": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-eslint/eslint-plugin": "^3.13.1",
    "@next/eslint-plugin-next": "^13.1.6",
    "@svgr/webpack": "^7.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-grid-layout": "^1.3.2",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "amplify-graphql-docs-generator": "^2.2.4",
    "assert": "^2.0.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "graphql-introspection-json-to-sdl": "^1.0.3",
    "husky": "^4.3.6",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.4",
    "sharp": "^0.32.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.9.5",
    "util": "^0.12.5"
  }
}
