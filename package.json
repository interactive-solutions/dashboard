{
  "name": "nextjs-boilerplate",
  "version": "1.3.37",
  "private": true,
  "author": "gunnarx2",
  "license": "MODO",
  "scripts": {
    "start": "next dev -p 9000",
    "start:localhost": "env-cmd -f .env.isLocalhost next dev -p 9000",
    "start:staging": "env-cmd -f .env.isStaging next dev -p 9000",
    "start:production": "env-cmd -f .env.isProduction next dev -p 9000",
    "build": "next build",
    "build:localhost": "env-cmd -f .env.isLocalhost next build",
    "build:staging": "env-cmd -f .env.isStaging next build",
    "build:production": "env-cmd -f .env.isProduction next build",
    "serve": "next start -p 9090",
    "serve:localhost": "env-cmd -f .env.isLocalhost next start -p 9090",
    "serve:staging": "env-cmd -f .env.isStaging next start -p 9090",
    "serve:production": "env-cmd -f .env.isProduction next start -p 9090",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run --reporter junit --reporter-options mochaFile=cypress/junit.xml,toConsole=true",
    "cypress:start:localhost": "start-server-and-test start:localhost http://localhost:9000 cypress:open",
    "cypress:start:staging": "start-server-and-test start:staging http://localhost:9000 cypress:open",
    "cypress:ci:localhost": "env-cmd -f .env.isLocalhost start-server-and-test start http://localhost:9000 cypress:run",
    "cypress:ci:staging": "env-cmd -f .env.isStaging start-server-and-test start http://localhost:9000 cypress:run",
    "prettier:report": "prettier '**/*.{js,tsx,ts,scss,json,md}' --check",
    "prettier:fix": "prettier '**/*.{js,tsx,ts,scss,json,md}' --write",
    "eslint:report": "eslint '**/*.{js,tsx,ts}' --cache",
    "eslint:fix": "eslint '**/*.{js,tsx,ts}' --fix",
    "stylelint:report": "stylelint '**/*.{js,jsx,ts,tsx}' --cache",
    "stylelint:fix": "stylelint '**/*.{js,jsx,ts,tsx}' --fix",
    "typescript:report": "tsc --noEmit && tsc --noEmit --project cypress/tsconfig.json",
    "graphql:generate": "graphql-codegen --config codegen/default.config.ts && yarn graphql:operations && graphql-codegen --config codegen/everything.config.ts",
    "graphql:watch": "graphql-codegen --config codegen/default.config.ts --watch",
    "graphql:operations": "amplify-graphql-docs-generator --schema src/graphql/cli/schema.everything.json --maxDepth=4 --output src/graphql/cli/operations.everything.graphql --language graphql",
    "storybook:start": "env-cmd -f .env.isStorybook start-storybook -p 1337",
    "storybook:build": "env-cmd -f .env.isStorybook build-storybook",
    "i18n:extract": "formatjs extract 'src/**/*.{js,tsx,ts}' --out-file src/i18n/cli/extracted.json --ignore **/*.d.ts",
    "i18n:compile": "formatjs compile src/i18n/cli/extracted.json --out-file src/i18n/cli/compiled.json"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@entire.se/hooks": "^1.3.2",
    "@entire.se/utils": "^1.1.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.9",
    "@sentry/nextjs": "^7.37.2",
    "@sentry/tracing": "^7.37.2",
    "apollo-upload-client": "^17.0.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^9.0.3",
    "graphql": "16.6.0",
    "next": "^13.1.6",
    "next-query-params": "^4.1.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-intl": "^6.2.8",
    "use-query-params": "^2.2.0",
    "validator": "^13.9.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@calm/eslint-plugin-react-intl": "^1.4.1",
    "@entire.se/eslint-config-eslint-react": "^1.2.2",
    "@entire.se/eslint-plugin-eslint-react": "^1.1.1",
    "@entire.se/prettier": "^1.1.1",
    "@formatjs/cli": "^6.0.1",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/fragment-matcher": "^4.0.0",
    "@graphql-codegen/introspection": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-eslint/eslint-plugin": "^3.16.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@sentry/cli": "^2.12.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@svgr/webpack": "^6.5.1",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/validator": "^13.7.12",
    "amplify-graphql-docs-generator": "^2.2.4",
    "assert": "^2.0.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cypress": "^11.2.0",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql-introspection-json-to-sdl": "^1.0.3",
    "husky": "^4.3.6",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sharp": "^0.31.3",
    "start-server-and-test": "^1.15.4",
    "storybook-addon-next": "^1.7.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-prettier": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.4.2",
    "util": "^0.12.5"
  },
  "resolutions": {
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.0",
    "webpack": "^5"
  }
}
